assName={styles.BlockContainer}>\n                <h2 className={styles.BlockTitle}>Проблема / Решение</h2>\n                <div className={styles.BlockText}>\n                    <p className={styles.Paragraph}>Маркетинг пришел с проблемой, юзеры уходят при добавлении новых мессенжеров</p>\n                    <p className={styles.Paragraph}>&nbsp;</p>\n                    <p className={styles.Paragraph}>Я провёл короткие интервью с теми, кто бросал процесс. Основной страх – люди не понимали, кто увидит их переписки и куда могут уйти пароли. Недоверие возникало ровно в моменте ввода данных.</p>\n                    <p className={styles.Paragraph}>&nbsp;</p>\n                    <p className={styles.Paragraph}>Решение: Добавил баннер, где объяснили пользователям, что их данные зашифрованы и у платформы нет к ним доступа</p>\n                </div>\n            </div>\n\n            <div className={styles.VideoBlock}>\n                <div className={styles.VideoContainer}>\n                    <video\n                        className={styles.VideoElement}\n                        autoPlay\n                        controlsList=\"nodownload\"\n                        loop\n                        muted\n                    >\n                        <source src=\"/Chatlab.webm\" type=\"video/webm\" />\n                        Ваш браузер не поддерживает видео\n                    </video>\n                </div>\n            </div>\n        </div>\n    );\n};\n\nexport default Chatlab;"],"mappings":"AAAA,MAAO,CAAAA,KAAK,KAAM,OAAO,CACzB,MAAO,CAAAC,MAAM,KAAM,4BAA4B,CAAC,OAAAC,GAAA,IAAAC,IAAA,CAAAC,IAAA,IAAAC,KAAA,yBAGhD,KAAM,CAAAC,OAAO,CAAGA,CAAA,GAAM,CAClB,mBACID,KAAA,QAAKE,SAAS,CAAEN,MAAM,CAACO,OAAQ,CAAAC,QAAA,eAC3BJ,KAAA,QAAKE,SAAS,CAAEN,MAAM,CAACS,cAAe,CAAAD,QAAA,eAClCN,IAAA,OAAII,SAAS,CAAEN,MAAM,CAACU,UAAW,CAAAF,QAAA,CAAC,0BAAmB,CAAI,CAAC,cAC1DN,IAAA,MAAGI,SAAS,CAAEN,MAAM,CAACW,gBAAiB,CAAAH,QAAA,CAAC,qOAEvC,CAAG,CAAC,EACH,CAAC,cAENJ,KAAA,QAAKE,SAAS,CAAEN,MAAM,CAA